{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/react" },
  "defaultProject": "presentation-web--frontend",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": { "style": "scss" },
      "library": { "style": "scss", "linter": "eslint" }
    }
  },
  "projects": {
    "presentation-web--frontend": {
      "root": "apps/presentation/web//frontend",
      "sourceRoot": "apps/presentation/web//frontend/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/presentation/web/frontend",
            "index": "apps/presentation/web/frontend/src/index.html",
            "main": "apps/presentation/web/frontend/src/main.tsx",
            "polyfills": "apps/presentation/web/frontend/src/polyfills.ts",
            "tsConfig": "apps/presentation/web/frontend/tsconfig.app.json",
            "assets": [
              "apps/presentation/web/frontend/src/favicon.ico",
              "apps/presentation/web/frontend/src/assets"
            ],
            "styles": [
              "apps/presentation/web/frontend/src/styles.scss"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/presentation/web/frontend/src/environments/environment.ts",
                  "with": "apps/presentation/web/frontend/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "presentation-web--frontend:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "presentation-web--frontend:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/presentation/web//frontend/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/presentation/web/frontend"],
          "options": {
            "jestConfig": "apps/presentation/web/frontend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "presentation/web/-frontend-e2e": {
      "root": "apps/presentation/web/frontend-e2e",
      "sourceRoot": "apps/presentation/web/frontend-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/presentation/web/frontend-e2e/cypress.json",
            "tsConfig": "apps/presentation/web/frontend-e2e/tsconfig.e2e.json",
            "devServerTarget": "presentation-web--frontend:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "presentation-web--frontend:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/presentation/web/frontend-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "presentation-web--api": {
      "root": "apps/presentation/web/api",
      "sourceRoot": "apps/presentation/web/api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/presentation/web/api",
            "main": "apps/presentation/web/api/src/main.ts",
            "tsConfig": "apps/presentation/web/api/tsconfig.app.json",
            "assets": ["apps/presentation/web/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/presentation/web/api/src/environments/environment.ts",
                  "with": "apps/presentation/web/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": { "buildTarget": "presentation-web--api:build" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/presentation/web/api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/presentation/web/api"],
          "options": {
            "jestConfig": "apps/presentation/web/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "domain-value-object": {
      "root": "libs/domain/value-object",
      "sourceRoot": "libs/domain/value-object/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/domain/value-object/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/domain/value-object"],
          "options": {
            "jestConfig": "libs/domain/value-object/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "infrastructure-repository": {
      "root": "libs/infrastructure/repository",
      "sourceRoot": "libs/infrastructure/repository/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/infrastructure/repository/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/infrastructure/repository"],
          "options": {
            "jestConfig": "libs/infrastructure/repository/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "domain-entity": {
      "root": "libs/domain/entity",
      "sourceRoot": "libs/domain/entity/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/domain/entity/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/domain/entity"],
          "options": {
            "jestConfig": "libs/domain/entity/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
